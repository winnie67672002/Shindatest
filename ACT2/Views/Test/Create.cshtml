@model ACT2.Models.TblSignupDo

@{
    ViewData["Title"] = "Create";
}
@{<div class="alert alert-danger" role="alert">
    @ViewData["error"]
</div>
}
<h1>Create</h1>

<h4>TblSignup</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="CName" class="control-label"></label>
                <input asp-for="CName" class="form-control" />
                <span asp-validation-for="CName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CMobile" class="control-label"></label>
                <input asp-for="CMobile" class="form-control" />
                <span asp-validation-for="CMobile" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CEmail" class="control-label"></label>
                <input asp-for="CEmail" class="form-control" />
                <span asp-validation-for="CEmail" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CCreateDt" class="control-label"></label>
                <input asp-for="CCreateDt" class="form-control" />
                <span asp-validation-for="CCreateDt" class="text-danger"></span>
            </div>
            <div class="form-group">
                <select asp-for ="queeryActive" class="control-label" multiple="multiple">
                @foreach (var item in ACT2.Utility.DDL.GetActiveItem() as List<TblActiveItem>)
                {
                    <option value="@item.CItemId">@item.CItemName</option>
                }
                </select>
           
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

